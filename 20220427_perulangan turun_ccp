#include <iostream>

using namespace std;
int main(){
    int number1 = 0;
    while(number1++ <10){ //perulangan turun
        cout<<"1 "<<" * "<<number1<<" = "<<number1*1<<endl;
    }
    cout<<endl<<endl;
    int number3 = 0;
    while(number3++ <10){ //perulangan turun
        cout<<"3 "<<" * "<<number3<<" = "<<number3*3<<endl;
    }
    cout<<endl<<endl;
    int number4 = 0;
    while(number4++ <10){ //perulangan turun
        cout<<"4 "<<" * "<<number4<<" = "<<number4*4<<endl;
    }
    

    return 0;
}
